#include <iostream>     
using namespace std;
 
void sort(int length,int sortarray[])
{
    while(length--)                         
   {                                         
	 bool swapped = false;                     
	 for(int i = 0; i < length; i++)  
	   {
          if(sortarray[i] > sortarray[i + 1])
		    {
			 swap(sortarray[i], sortarray[i + 1]);
			 swapped = true;
			}
	    }
	 if(swapped == false)
	 break;
	 } 
}
 
int main()
{
   int size, sizestr, n;
   
   cout<<"Enter size of array: ";
   cin>>size;
   cout<<"Enter length of string: ";
   cin>>sizestr;
   
   char* str = new char[sizestr];      
   char** arraychar = new char*[size];
   for ( int i = 0; i < size; i++) 
        arraychar[i] = new char[sizestr];
   int* arrayint = new int[size];

  for(int i = 0; i < size; i++)    
    {
        cin>>str;    
        for(int j = 0; j < sizestr; j++)   
        {
           arraychar[i][j]=str[j];
        };
        str[0]=0;
    }
  
   for (int i = 0; i < size; i++)  
   {				
    n=0;
    while (arraychar[i][n]!='\0')
    {
        arrayint[i]=10*arrayint[i]+(arraychar[i][n]-'0');
        n+=1;
    }
   };
   
   sort(size,arrayint);
   
   for (int i = 0; i < size; ++i)     
   {
    cout<< *(arrayint + i) << " ";
   };
   
   delete[] str;
   delete[] arraychar;
   delete[] arrayint;     
   return 0;
}
